  fhir:description [ fhir:v "This is a plandefinition for Pediatrics cohort protocol"] ; # 
  fhir:action ( [
     fhir:prefix [ fhir:v "1.1 - 1.6" ] ;
     fhir:title [ fhir:v "Required task across all chorts" ] ;
     fhir:description [ fhir:v "List of tasks and actions done across all 5 cohorts" ] ;
     fhir:priority [ fhir:v "routine" ] ;
     fhir:definition [ fhir:v "https://voicecollab.ai/fhir/PlanDefinition/VbaiPlanDefinitionExample"^^xsd:anyURI ]
  ] [
     fhir:prefix [ fhir:v "B" ] ;
     fhir:title [ fhir:v "Part B Peds" ] ;
     fhir:description [ fhir:v "Tasks specific to Peds cohort ​" ] ;
     fhir:priority [ fhir:v "routine" ] ;
     fhir:action ( [
       fhir:prefix [ fhir:v "B.1 Task 1" ] ;
       fhir:title [ fhir:v "Produce sustained /a/ sound " ] ;
       fhir:description [ fhir:v "    \n    a) App demonstration first \n\n    b) At a comfortable pitch and volume using a normal speaking voice \n\n    C) Duration: 4 seconds " ]
     ] [
       fhir:prefix [ fhir:v "B.1 Task 2" ] ;
       fhir:title [ fhir:v "Diadochokinetic task with “patty-cake”" ] ;
       fhir:description [ fhir:v "Record the time that it takes to repeat the syllables \n    'pat' 10 times \n\n    'tee' 10 times \n\n    'cake' 10 times \n\n    Repeat whole word as fast as possible for 30 seconds " ]
     ] [
       fhir:prefix [ fhir:v "B.1 Task 3" ] ;
       fhir:title [ fhir:v "Recite the alphabet (ABC’s)" ] ;
       fhir:description [ fhir:v "Duration: until complete, or for 60 seconds" ]
     ] [
       fhir:prefix [ fhir:v "B.1 Task 4" ] ;
       fhir:title [ fhir:v "Count as high as you can" ] ;
       fhir:description [ fhir:v "Duration: 60 seconds " ]
     ] [
       fhir:prefix [ fhir:v "B.1 Task 5" ] ;
       fhir:title [ fhir:v "Name daily life pictures" ] ;
       fhir:description [ fhir:v "App prompts child to name 36 randomly-selected pictures\n\n    Duration of task: 60 seconds\n\n    Modification if unable to see: repeat after adult" ]
     ] [
       fhir:prefix [ fhir:v "B.1 Task 6" ] ;
       fhir:title [ fhir:v "Read or repeat CAPE-V sentences:" ] ;
       fhir:description [ fhir:v "Please read the following sentences one at a time, as if you were speaking to somebody in a real conversation.” If the individual has difficulty reading, the clinician may ask him or her to repeat sentences after verbal examples. App to show sentences appearing one at a time. \n\n    “The blue spot is on the key again”\n\n    “How hard did he hit him?” \n\n    “We were away a year ago”\n\n    “We eat eggs every Easter”\n\n    “My mama makes lemon jam”\n\n    “Peter will keep at the peak”" ]
     ] [
       fhir:prefix [ fhir:v "B.1 Task 7" ] ;
       fhir:title [ fhir:v "Read or repeat the Caterpillar Passage – in app, passage will appear one sentence at a time " ] ;
       fhir:description [ fhir:v "Do you like amusement parks? Well, I sure do. To amuse myself, I went twice last spring. My most MEMORABLE moment was riding on the Caterpillar, which is a gigantic roller coaster high above the ground. When I saw how high the Caterpillar rose into the bright blue sky I knew it was for me. After waiting in line for thirty minutes, I made it to the front where the man measured my height to see if I was tall enough. I gave the man my coins, asked for change, and jumped on the cart. Tick, tick, tick, the Caterpillar climbed slowly up the tracks. It went SO high I could see the parking lot. Boy was I SCARED! I thought to myself, “There’s no turning back now.” People were so scared they screamed as we swiftly zoomed fast, fast, and faster along the tracks. As quickly as it started, the Caterpillar came to a stop. Unfortunately, it was time to pack the car and drive home. That night I dreamt of the wild ride on the Caterpillar. Taking a trip to the amusement park and riding on the Caterpillar was my MOST memorable moment ever!” \n    Modification: recite passage sentence by sentence after adult" ]
     ] [
       fhir:prefix [ fhir:v "B.1 Task 8" ] ;
       fhir:title [ fhir:v "Cued conversation:" ] ;
       fhir:description [ fhir:v " If child responds, record for 60 seconds and then end protocol  \n    If preferred toy present:  \n\n    “what is that?” \n\n    “what can it do?” \n\n    “does it make any sounds?” \n\n    “why do you like it?” \n\nIf preferred toy not present:  \n\n    “What is your favorite animal?” \n\n    “what does it sound like?” \n\n    “what can it do?”  \n\n    “why do you like it?” \n\nIf child does not respond, proceed to #9 " ]
     ] [
       fhir:prefix [ fhir:v "B.1 Task 9" ] ;
       fhir:title [ fhir:v "Spontaneous vocalizations/cry sounds " ] ;
       fhir:description [ fhir:v "If preferred toy present:  \n\n    “I like your toy! Does your toy have a name?” \n\n    “what does your toy say?” \n\n    “what does your toy do?” (parent encourages child to play with toy) \n\n    If preferred toy not present, bring out animal toys (barn setup):  \n\n    Parent and child sit down together \n\n    Child picks up animal toys \n\n    Parent makes animal sounds with each toy the child picks up \n\nParent may also want to move animals around the barn scene if child not engaging with the toys and make noises with each animal they hold \n\n    Hopefully, this inspires child to vocalize, too \n\nRecord for 60 seconds and then end protocol " ]
     ] )
  ] ) . # 

